\newcommand\SHOW[1]{%
  \def\UnicodeMathSymbol##1##2##3##4{%
    \def\1{#1}\def\2{##3}%
    \ifx\1\2\PRINTLINE{##1}{##2}{##4}\fi
  }%
  \TABLE
}
\ExplSyntaxOn
\def\PRINTLINE#1#2#3{
    \def\tempa{
      \USV{#1} &
      \SYMB{#2}{lm}{#1} &
      \SYMB{#2}{xits}{#1} &
      \SYMB{#2}{stix}{#1} &
      \SYMB{#2}{pagella}{#1} &
      \SYMB{#2}{dejavu}{#1} &
      \SYMB{#2}{fira}{#1} &
      \CMD{#2}
      \tl_if_in:NnT \PLAIN {#2}
        {
          \makebox[0pt][l]
            { \color[gray]{0.7} \textsuperscript{\sffamily (p)} }
        }
      \tl_if_in:NnT \LTXSYM {#2}
        {
          \makebox[0pt][l]
            { \color[gray]{0.7} \textsuperscript{\sffamily (l)} }
        }
      \tl_if_in:NnT \AMSSYMB {#2}
        {
          \makebox[0pt][l]
            { \color[gray]{0.7} \textsuperscript{\sffamily (a)} }
        }
      &
      \DESC{#3} \\
    }%
    \expandafter\tempa
}
\ExplSyntaxOff
\def\SYMB#1#2#3{%
  \expandafter\iffontchar\csname#2\endcsname #3\relax
    \refstepcounter{#2}%
    \mathversion{#2}%
    $\displaystyle#1$%
  \fi
}

